  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/consul": "^0.23.33",
    "@types/dotenv": "^6.1.0",
    "@types/glob": "^5.0.35",
    "@types/jest": "^23.3.0",
    "@types/lodash": "^4.14.123",
    "@types/uuid": "^3.4.4",
    "coveralls": "^3.0.2",

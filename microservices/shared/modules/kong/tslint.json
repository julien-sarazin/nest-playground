{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-unused-expression": true
    },
    "rules": {
      "variable-name": false,
      "no-empty-interface": false,
        "interface-over-type-literal": false,
      "max-classes-per-file": false,
      "quotemark": [
            true,
            "single"
        ],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            150
        ],
        "member-ordering": [
            false
        ],
        "one-line": false,
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-console": false
    },
    "rulesDirectory": []
}
